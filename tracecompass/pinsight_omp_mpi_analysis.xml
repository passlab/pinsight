<?xml version="1.0" encoding="UTF-8"?>
<tmfxml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="xmldefinition.xsd">

	<!-- XSD definition: https://github.com/tracecompass/tracecompass/tree/master/tmf/org.eclipse.tracecompass.tmf.analysis.xml.core/src/org/eclipse/tracecompass/tmf/analysis/xml/core/module -->

	<!-- The state provider assigns states from events -->
	<stateProvider id="xxx.pinsight.stateProvider" version="0">
		<head>
			<traceType id="org.eclipse.linuxtools.lttng2.ust.tracetype" />
			<label value="PInsight OpenMP and MPI Trace Analysis" />
		</head>

		<!-- In sequential compute state, the first and last state pinsight tracks -->
		<definedValue name="STATE_IN_SEQ_COMPUTE" value="100" />
		<definedValue name="STATE_SEQ_TERMINATED" value="101" />

		<!-- Convenience names for the thread state values -->
		<definedValue name="STATE_OMP_THREAD_STARTED" value="201" />
		<definedValue name="STATE_OMP_TRANSITION" value="209" /> <!-- changing state -->

		<!-- 220 - 249 are for those states in which threads are doing work -->
		<definedValue name="STATE_OMP_PARALLEL" value="220" />
		<definedValue name="STATE_IN_OMP_IMPLICIT_TASK" value="221" />
		<definedValue name="STATE_IN_OMP_WORK" value="222" />
		<definedValue name="STATE_IN_OMP_MASTER" value="227" />

		<!-- 250 - 269 are for those states in which threads are synchronizing -->
		<definedValue name="STATE_OMP_SYNC" value="250" />
		<definedValue name="STATE_IN_OMP_SYNC_WAIT" value="251" />
		<definedValue name="STATE_OMP_JOIN_SYNC" value="252" />
		<definedValue name="STATE_IN_OMP_JOIN_SYNC_WAIT" value="253" />
		<definedValue name="STATE_OMP_BARRIER_SYNC" value="254" />
		<definedValue name="STATE_IN_OMP_BARRIER_SYNC_WAIT" value="255" />

		<definedValue name="STATE_OMP_THREAD_TERMINATED" value="300" />
		<definedValue name="STATE_DONOT_CARE" value="301" /> <!-- do not care what happen in this, set it as white -->

		<!-- Convenience names for the MPI state values -->
		<definedValue name="STATE_MPI_Inited" value="400" /> <!-- right after MPI_Init call -->
		<definedValue name="STATE_IN_COMPUTE_AFTER_MPI" value="401" /> <!-- right after an MPI call -->

		<!-- 220 - 249 are for those states in which threads are doing work -->
		<definedValue name="STATE_IN_MPI_Send" value="420" />
		<definedValue name="STATE_IN_MPI_Recv" value="421" />
		<definedValue name="STATE_IN_MPI_Sendrecv" value="422" />
		<definedValue name="STATE_IN_MPI_Isend" value="423" />
		<definedValue name="STATE_IN_MPI_Irecv" value="424" />
		<definedValue name="STATE_IN_MPI_Wait" value="425" />
		<definedValue name="STATE_IN_MPI_Waitall" value="426" />
		<definedValue name="STATE_IN_MPI_Waitany" value="427" />
		<definedValue name="STATE_IN_MPI_Waitsome" value="428" />

		<definedValue name="STATE_IN_MPI_Allgather" value="430" />
		<definedValue name="STATE_IN_MPI_Allreduce" value="431" />
		<definedValue name="STATE_IN_MPI_Alltoall" value="432" />
		<definedValue name="STATE_IN_MPI_Barrier" value="433" />
		<definedValue name="STATE_IN_MPI_Bcast" value="434" />
		<definedValue name="STATE_IN_MPI_Gather" value="435" />
		<definedValue name="STATE_IN_MPI_Reduce" value="436" />
		<definedValue name="STATE_IN_MPI_Scatter" value="437" />
		<definedValue name="STATE_IN_MPI_Reduce_scatter" value="438" />

		<definedValue name="STATE_IN_MPI_Accumulate" value="440" />
		<definedValue name="STATE_IN_MPI_Get" value="441" />
		<definedValue name="STATE_IN_MPI_Put" value="442" />

		<definedValue name="STATE_IN_MPI_Finalize" value="498" />
		<definedValue name="STATE_MPI_Finalized" value="499" /> <!-- right after MPI finalized -->

		<!-- Event handlers for thread timeGraph view -->
		<!-- Event handlers for thread-CPU timeGraph view -->
		<!-- Event handlers for CPU-state timeGraph view -->
		<!-- Event handlers for parallel region timeGraph view-->
		<!-- Event handlers for instances of parallel regions timeGraph view-->

		<location id="HostProcessThread">
			<stateAttribute type="constant" value="Status of Host:Process:OSThread" />
			<stateAttribute type="constant" value="Host" />
			<stateAttribute type="eventField" value="hostname" />
			<stateAttribute type="constant" value="Process" />
			<stateAttribute type="eventField" value="pid" />
			<stateAttribute type="constant" value="Thread" />
			<stateAttribute type="eventField" value="context._vtid" />
		</location>

		<location id="HostMPIProcessOMPThread">
			<stateAttribute type="constant" value="Status of Host:MPIProcess:OMPThread" />
			<stateAttribute type="constant" value="Host" />
			<stateAttribute type="eventField" value="hostname" />
			<stateAttribute type="constant" value="MPI Process" />
			<stateAttribute type="eventField" value="mpirank" />
			<stateAttribute type="constant" value="Thread" />
			<stateAttribute type="eventField" value="global_thread_num" />
		</location>

		<location id="MPIProcessOMPThread">
			<stateAttribute type="constant" value="Status of MPIProcess:OMPThread" />
			<stateAttribute type="constant" value="MPI Process" />
			<stateAttribute type="eventField" value="mpirank" />
			<stateAttribute type="constant" value="Thread" />
			<stateAttribute type="eventField" value="global_thread_num" />
		</location>

		<location id="ParallelRegionHostMPIProcessOMPThread">
			<stateAttribute type="constant" value="Status of ParallelRegion:Host:MPIProcess:OMPThread" />
			<stateAttribute type="constant" value="parallel_region" />
			<stateAttribute type="eventField" value="parallel_codeptr" />
			<stateAttribute type="constant" value="Host" />
			<stateAttribute type="eventField" value="hostname" />
			<stateAttribute type="constant" value="MPI Process" />
			<stateAttribute type="eventField" value="mpirank" />
			<stateAttribute type="constant" value="OpenMP Thread" />
			<stateAttribute type="eventField" value="omp_thread_num" />
		</location>

		<eventHandler eventName="lttng_pinsight_enter_exit:enter_pinsight">
			<stateChange>
				<stateAttribute type="location" value="HostProcessThread"/>
				<stateValue type="int" value="$STATE_IN_SEQ_COMPUTE" />
			</stateChange>
		</eventHandler>

		<eventHandler eventName="lttng_pinsight_enter_exit:exit_pinsight">
			<stateChange>
				<stateAttribute type="location" value="HostProcessThread"/>
				<stateValue type="int" value="$STATE_SEQ_TERMINATED" />
			</stateChange>
		</eventHandler>

		<eventHandler eventName="ompt_pinsight_lttng_ust:thread_begin">
			<stateChange>
				<stateAttribute type="location" value="HostProcessThread"/>
				<stateValue type="int" value="$STATE_OMP_THREAD_STARTED" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="HostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_OMP_THREAD_STARTED" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="MPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_OMP_THREAD_STARTED" />
			</stateChange>
		</eventHandler>

		<eventHandler eventName="ompt_pinsight_lttng_ust:thread_end">
			<stateChange>
				<stateAttribute type="location" value="HostProcessThread"/>
				<stateValue type="int" value="$STATE_OMP_THREAD_TERMINATED" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="HostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_OMP_THREAD_TERMINATED" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="MPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_OMP_THREAD_TERMINATED" />
			</stateChange>
		</eventHandler>

		<!-- TODO: not considering nest parallelism right now.
		     For nested parallelism, current state value should be pushed to a stack on parallel_begin, and
		     then new value is assigned as the current state
		-->
		<eventHandler eventName="ompt_pinsight_lttng_ust:parallel_begin">
			<stateChange>
				<stateAttribute type="location" value="HostProcessThread"/>
				<stateValue type="int" value="$STATE_OMP_PARALLEL" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="HostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_OMP_PARALLEL" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="MPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_OMP_PARALLEL" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="ParallelRegionHostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_OMP_PARALLEL" />
			</stateChange>
		</eventHandler>

		<!-- TODO: not considering nest parallelism right now.
		     For nested parallelism, state value should be set as the one that is popped from the stack
		-->
		<eventHandler eventName="ompt_pinsight_lttng_ust:parallel_end">
			<stateChange>
				<stateAttribute type="location" value="HostProcessThread"/>
				<stateValue type="int" value="$STATE_IN_SEQ_COMPUTE" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="HostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_IN_SEQ_COMPUTE" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="MPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_IN_SEQ_COMPUTE" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="ParallelRegionHostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_IN_SEQ_COMPUTE" />
			</stateChange>
		</eventHandler>

		<eventHandler eventName="ompt_pinsight_lttng_ust:parallel_join_sync_begin">
			<stateChange>
				<stateAttribute type="location" value="HostProcessThread"/>
				<stateValue type="int" value="$STATE_OMP_JOIN_SYNC" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="HostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_OMP_JOIN_SYNC" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="MPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_OMP_JOIN_SYNC" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="ParallelRegionHostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_OMP_JOIN_SYNC" />
			</stateChange>
		</eventHandler>

		<eventHandler eventName="ompt_pinsight_lttng_ust:parallel_join_sync_wait_begin">
			<stateChange>
				<stateAttribute type="location" value="HostProcessThread"/>
				<stateValue type="int" value="$STATE_IN_OMP_JOIN_SYNC_WAIT" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="HostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_IN_OMP_JOIN_SYNC_WAIT" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="MPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_IN_OMP_JOIN_SYNC_WAIT" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="ParallelRegionHostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_IN_OMP_JOIN_SYNC_WAIT" />
			</stateChange>
		</eventHandler>

		<eventHandler eventName="ompt_pinsight_lttng_ust:parallel_join_sync_wait_end">
			<stateChange>
				<stateAttribute type="location" value="HostProcessThread"/>
				<stateValue type="int" value="$STATE_OMP_TRANSITION" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="HostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_OMP_TRANSITION" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="MPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_OMP_TRANSITION" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="ParallelRegionHostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_OMP_TRANSITION" />
			</stateChange>
		</eventHandler>

		<eventHandler eventName="ompt_pinsight_lttng_ust:parallel_join_sync_end">
			<stateChange>
				<stateAttribute type="location" value="HostProcessThread"/>
				<stateValue type="int" value="$STATE_OMP_TRANSITION" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="HostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_OMP_TRANSITION" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="MPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_OMP_TRANSITION" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="ParallelRegionHostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_OMP_TRANSITION" />
			</stateChange>
		</eventHandler>

		<eventHandler eventName="ompt_pinsight_lttng_ust:parallel_barrier_sync_begin">
			<stateChange>
				<stateAttribute type="location" value="HostProcessThread"/>
				<stateValue type="int" value="$STATE_OMP_BARRIER_SYNC" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="HostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_OMP_BARRIER_SYNC" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="MPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_OMP_BARRIER_SYNC" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="ParallelRegionHostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_OMP_BARRIER_SYNC" />
			</stateChange>
		</eventHandler>

		<eventHandler eventName="ompt_pinsight_lttng_ust:parallel_barrier_sync_wait_begin">
			<stateChange>
				<stateAttribute type="location" value="HostProcessThread"/>
				<stateValue type="int" value="$STATE_IN_OMP_BARRIER_SYNC_WAIT" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="HostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_IN_OMP_BARRIER_SYNC_WAIT" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="MPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_IN_OMP_BARRIER_SYNC_WAIT" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="ParallelRegionHostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_IN_OMP_BARRIER_SYNC_WAIT" />
			</stateChange>
		</eventHandler>

		<eventHandler eventName="ompt_pinsight_lttng_ust:parallel_barrier_sync_wait_end">
			<stateChange>
				<stateAttribute type="location" value="HostProcessThread"/>
				<stateValue type="int" value="$STATE_OMP_TRANSITION" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="HostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_OMP_TRANSITION" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="MPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_OMP_TRANSITION" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="ParallelRegionHostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_OMP_TRANSITION" />
			</stateChange>
		</eventHandler>

		<eventHandler eventName="ompt_pinsight_lttng_ust:parallel_barrier_sync_end">
			<stateChange>
				<stateAttribute type="location" value="HostProcessThread"/>
				<stateValue type="int" value="$STATE_OMP_TRANSITION" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="HostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_OMP_TRANSITION" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="MPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_OMP_TRANSITION" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="ParallelRegionHostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_OMP_TRANSITION" />
			</stateChange>
		</eventHandler>

		<eventHandler eventName="ompt_pinsight_lttng_ust:implicit_task_begin">
			<stateChange>
				<stateAttribute type="location" value="HostProcessThread"/>
				<stateValue type="int" value="$STATE_IN_OMP_IMPLICIT_TASK" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="HostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_IN_OMP_IMPLICIT_TASK" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="MPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_IN_OMP_IMPLICIT_TASK" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="ParallelRegionHostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_IN_OMP_IMPLICIT_TASK" />
			</stateChange>
		</eventHandler>

		<eventHandler eventName="ompt_pinsight_lttng_ust:implicit_task_end">
			<stateChange>
				<stateAttribute type="location" value="HostProcessThread"/>
				<stateValue type="int" value="$STATE_OMP_TRANSITION" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="HostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_OMP_TRANSITION" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="MPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_OMP_TRANSITION" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="ParallelRegionHostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_OMP_TRANSITION" />
			</stateChange>
		</eventHandler>


		<eventHandler eventName="ompt_pinsight_lttng_ust:work_begin">
			<stateChange>
				<stateAttribute type="location" value="HostProcessThread"/>
				<stateValue type="int" value="$STATE_IN_OMP_WORK" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="HostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_IN_OMP_WORK" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="MPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_IN_OMP_WORK" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="ParallelRegionHostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_IN_OMP_WORK" />
			</stateChange>
		</eventHandler>

		<!-- after a work (worksharing loop/for, section, single, etc) ends, thread goes back to implicit task
		     of the enclosing parallel region, thus it returns to the state of $STATE_IN_OMP_IMPLICIT_TASK.
		     If we have stack, we can push/pop of the state value upon work_begin and work_end event
		-->
		<eventHandler eventName="ompt_pinsight_lttng_ust:work_end">
			<stateChange>
				<stateAttribute type="location" value="HostProcessThread"/>
				<stateValue type="int" value="$STATE_IN_OMP_IMPLICIT_TASK" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="HostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_IN_OMP_IMPLICIT_TASK" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="MPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_IN_OMP_IMPLICIT_TASK" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="ParallelRegionHostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_IN_OMP_IMPLICIT_TASK" />
			</stateChange>
		</eventHandler>

		<eventHandler eventName="ompt_pinsight_lttng_ust:masked_begin">
			<stateChange>
				<stateAttribute type="location" value="HostProcessThread"/>
				<stateValue type="int" value="$STATE_IN_OMP_MASTER" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="HostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_IN_OMP_MASTER" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="MPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_IN_OMP_MASTER" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="ParallelRegionHostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_IN_OMP_MASTER" />
			</stateChange>
		</eventHandler>

		<!-- after a masked, thread goes back to the implicit task -->
		<eventHandler eventName="ompt_pinsight_lttng_ust:masked_end">
			<stateChange>
				<stateAttribute type="location" value="HostProcessThread"/>
				<stateValue type="int" value="$STATE_IN_OMP_IMPLICIT_TASK" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="HostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_IN_OMP_IMPLICIT_TASK" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="MPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_IN_OMP_IMPLICIT_TASK" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="ParallelRegionHostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_IN_OMP_IMPLICIT_TASK" />
			</stateChange>
		</eventHandler>

		<!-- ============================ MPI event Handler =========================== -->
		<!-- Event handlers for process timeGraph view -->
		<!-- Event handlers for process-CPU timeGraph view -->
		<!-- Event handlers for CPU-state timeGraph view -->
		<!-- Event handlers for each MPI call handler -->
		<eventHandler eventName="pmpi_pinsight_lttng_ust:MPI_Init_thread_end">
			<stateChange>
				<stateAttribute type="location" value="HostProcessThread"/>
				<stateValue type="int" value="$STATE_IN_COMPUTE_AFTER_MPI" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="HostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_IN_COMPUTE_AFTER_MPI" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="MPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_IN_COMPUTE_AFTER_MPI" />
			</stateChange>
		</eventHandler>
		<eventHandler eventName="pmpi_pinsight_lttng_ust:MPI_Init_end">
			<stateChange>
				<stateAttribute type="location" value="HostProcessThread"/>
				<stateValue type="int" value="$STATE_IN_COMPUTE_AFTER_MPI" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="HostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_IN_COMPUTE_AFTER_MPI" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="MPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_IN_COMPUTE_AFTER_MPI" />
			</stateChange>
		</eventHandler>

		<eventHandler eventName="pmpi_pinsight_lttng_ust:MPI_Finalize_begin">
			<stateChange>
				<stateAttribute type="location" value="HostProcessThread"/>
				<stateValue type="int" value="$STATE_IN_MPI_Finalize" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="HostMPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_IN_MPI_Finalize" />
			</stateChange>
			<stateChange>
				<stateAttribute type="location" value="MPIProcessOMPThread"/>
				<stateValue type="int" value="$STATE_IN_MPI_Finalize" />
